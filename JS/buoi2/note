  <!-- 
 cho a = 1; b = 2, c = b++ - --b + a++ - b-- + ++a . Tìm a = ?, b = ? và c = ?
 
 */
        a = 1 
        b = 2
        c = b++ - --b + a++ - b-- + ++a
        c = 2 - 2  + 1 - 2 + 3= 2 

        a =3
        
        b= 1

        21 * (53 + ‘5’ - '2') + 21 - '20' / ‘5’ - (11 + 2 - ‘6’ * 4)
        = 21 * (535 -2) + 21 - 4 -(13 -24)
        =11221

